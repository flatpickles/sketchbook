#ifndef FNC_QUINTIC
#define FNC_QUINTIC

float quintic(const in float v){return v*v*v*(v*(v*6.-15.)+10.);}
vec2 quintic(const in vec2 v){return v*v*v*(v*(v*6.-15.)+10.);}
vec3 quintic(const in vec3 v){return v*v*v*(v*(v*6.-15.)+10.);}
vec4 quintic(const in vec4 v){return v*v*v*(v*(v*6.-15.)+10.);}

#endif